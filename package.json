{
  "name": "transferapi",
  "version": "1.0.0",
  "description": "transfer api excersise",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && tsc -p ./tsconfig.build.json",
    "start": "node dist/server.js",
    "clean": "rm -rf ./dist && mkdir dist",
    "dev": "tsc-watch -p ./tsconfig.build.json --onSuccess \"node ./dist/server.js\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint-check": "eslint '*/**/*.{js,ts,tsx}' --quiet",
    "lint-fix": "eslint '*/**/*.{js,ts,tsx}' --fix"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css}": [
      "prettier --write",
      "eslint --fix src/",
      "git add"
    ]
  },
  "_moduleAliases": {
    "~api": "dist/api",
    "~config": "dist/config",
    "~constants": "dist/constants",
    "~db": "dist/db",
    "~libs": "dist/libs",
    "~loaders": "dist/loaders",
    "~routes": "dist/api/routes",
    "~utils": "dist/utils"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/salvador-lucas/transferApi.git"
  },
  "author": "lucas salvador",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/salvador-lucas/transferApi/issues"
  },
  "homepage": "https://github.com/salvador-lucas/transferApi#readme",
  "dependencies": {
    "@types/node": "^18.14.6",
    "ajv": "^8.11.0",
    "ajv-errors": "^3.0.0",
    "ajv-formats": "^2.1.1",
    "ajv-keywords": "^5.1.0",
    "axios": "^1.3.4",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.0",
    "cls-hooked": "^4.2.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "helmet": "^5.1.1",
    "jsonwebtoken": "^9.0.0",
    "moment": "^2.29.4",
    "mysql": "^2.18.1",
    "mysql2": "^3.2.0",
    "object-inspect": "^1.12.2",
    "sequelize": "^6.29.0"
  },
  "devDependencies": {
    "@types/ajv": "^1.0.0",
    "@types/ajv-errors": "^2.0.1",
    "@types/ajv-keywords": "^3.5.0",
    "@types/bcrypt": "^5.0.0",
    "@types/body-parser": "^1.19.2",
    "@types/cls-hooked": "^4.3.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/helmet": "^4.0.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/object-inspect": "^1.8.1",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "cd": "^0.3.3",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "module-alias": "^2.2.2",
    "prettier": "^2.7.1",
    "prettier-eslint": "^15.0.1",
    "tsc-watch": "^5.0.3",
    "typescript": "^4.7.4"
  }
}
